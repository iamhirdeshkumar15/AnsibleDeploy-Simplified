# Define the group of servers where tasks will be executed
[web_servers]
host1 ansible_host = private ip address
host2 ansible_host = private ip address

# Set up variables for all servers in the 'web_servers' group
[web_servers:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=/path/to/Ansi.pem
ansible_python_interpreter=/usr/bin/python3
